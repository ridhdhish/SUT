<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f8f8f8"
    android:orientation="vertical"
    tools:context="com.example.ridhdhish.sut.guest_flatholder">


    <android.support.v7.widget.Toolbar
        android:id="@+id/GuestFlatHolderToolBar"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:background="#a27ce6"
        app:titleTextColor="#ffffff"
        android:theme="@style/toolbarColor">

    </android.support.v7.widget.Toolbar>

    <TextView
        android:id="@+id/txtGuestFlatHolder"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:text="No data available"
        android:textSize="25dp"
        android:gravity="center"
        android:visibility="gone"/>

    <ListView
        android:id="@+id/listviewGuestUserFlatHolders"
        android:layout_width="match_parent"
        android:layout_height="match_parent" android:paddingLeft="5dp"/>

</LinearLayout>
